using System;

namespace newtask
{
    class Program
    {
        static void Main(string[] args)
        {
            int num = Convert.ToInt32(Console.ReadLine());
            int faktorial=1;
            while(num!=0)
            {
                faktorial *= num;
                num -= 1;
            }
            Console.WriteLine(faktorial);
        }
    } 
} 
